{
	"name": "@twyr/ember-mdc-web",
	"displayName": "Ember MDC Web",
	"description": "Ember v4.0+ implementation of material design components for the web",
	"homepage": "https://github.com/twyr/twyr-mdc-web#readme",
	"license": "MITNFA",
	"readme": "README.md",
	"version": "0.1.1",
	"keywords": [
		"ember-addon",
		"material",
		"design",
		"ember",
		"component",
		"suite",
		"style"
	],
	"author": {
		"name": "Vish Desai",
		"email": "shadyvd@hotmail.com",
		"url": "https://github.com/shadyvd"
	},
	"contributors": [
		{
			"name": "Vish Desai",
			"email": "shadyvd@hotmail.com",
			"url": "https://github.com/shadyvd"
		}
	],
	"maintainers": [
		{
			"name": "Vish Desai",
			"email": "shadyvd@hotmail.com",
			"url": "https://github.com/shadyvd"
		}
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/twyr/twyr-mdc-web.git"
	},
	"bugs": {
		"url": "https://github.com/twyr/twyr-mdc-web/issues"
	},
	"exports": {
		"./*": "./dist/*",
		"./addon-main.js": "./addon-main.js"
	},
	"directories": {
		"doc": "doc",
		"test": "tests"
	},
	"files": [
		"addon-main.js",
		"dist"
	],
	"dependencies": {
		"@embroider/addon-shim": "1.0.0",
		"@glimmer/component": "1.0.4",
		"@glimmer/tracking": "1.0.4",
		"@mdi/font": "6.5.95",
		"ember-cli-sass": "10.0.1",
		"ember-debug-logger": "2.1.1",
		"material-components-web": "13.0.0",
		"modern-normalize": "1.1.0",
		"sass": "1.49.7"
	},
	"devDependencies": {
		"@babel/core": "7.17.0",
		"@babel/eslint-parser": "7.17.0",
		"@babel/plugin-proposal-class-properties": "7.16.7",
		"@babel/plugin-proposal-decorators": "7.17.0",
		"@babel/plugin-syntax-decorators": "7.17.0",
		"@embroider/addon-dev": "1.0.0",
		"@rollup/plugin-babel": "5.3.0",
		"ember-template-lint": "4.0.0",
		"ember-template-lint-plugin-prettier": "4.0.0",
		"eslint": "8.8.0",
		"eslint-plugin-ember": "10.5.8",
		"eslint-plugin-json": "3.1.0",
		"eslint-plugin-node": "11.1.0",
		"prettier": "2.5.1",
		"prettier-plugin-jsdoc": "0.3.30",
		"stylelint": "14.3.0",
		"stylelint-config-hudochenkov": "6.0.1",
		"stylelint-config-prettier": "9.0.3",
		"stylelint-order": "5.0.0",
		"stylelint-prettier": "2.0.0",
		"stylelint-scss": "4.1.0",
		"rollup": "2.67.1"
	},
	"scripts": {
		"build": "mkdir -p ./dist && rm -rf ./dist && npx rollup --config && npx prettier --write . && npx eslint . --cache --fix",
		"lint": "npm run lint:hbs && npm run lint:css && npm run lint:js",
		"lint:fix": "npm run lint:hbs:fix && npm run lint:css:fix && npm run lint:js:fix",
		"lint:css": "npx stylelint ./**/*.scss --cache",
		"lint:css:fix": "npx stylelint ./**/*.scss --cache --fix",
		"lint:hbs": "npx ember-template-lint .",
		"lint:hbs:fix": "npx ember-template-lint . --fix",
		"lint:js": "npx eslint . --cache",
		"lint:js:fix": "npx prettier --write . && npx eslint . --cache --fix",
		"lint-staged": "npx lint-staged --allow-empty",
		"start": "mkdir -p ./dist && rm -rf ./dist && npx rollup --config --watch && npx prettier --write . && npx eslint . --cache --fix",
		"test": "echo 'Addon does not have tests, run tests in test-app'",
		"prepare": "mkdir -p ./dist && rm -rf ./dist && npx rollup --config && npx eslint . --cache --fix"
	},
	"engines": {
		"node": ">= 14"
	},
	"ember": {
		"edition": "octane"
	},
	"ember-addon": {
		"version": 2,
		"type": "addon",
		"main": "addon-main.js",
		"app-js": {
			"./components/mdc-card/index.js": "./dist/_app_/components/mdc-card/index.js",
			"./components/mdc-headline/index.js": "./dist/_app_/components/mdc-headline/index.js",
			"./components/mdc-sub-headline/index.js": "./dist/_app_/components/mdc-sub-headline/index.js"
		}
	}
}

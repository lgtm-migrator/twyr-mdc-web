<ul
	class='mdc-list'
	{{! Store reference to the element for DOM manipulation }}
	{{store-element this.storeElement}}
	{{! HTML attributes added in the calling template }}
	...attributes
>
	{{yield
		(hash
			Divider=(component this.dividerComponent palette=@palette)
			Item=(component
				this.listItemComponent
				doubleLine=@doubleLine
				palette=@palette
				shaped=@shaped
			)
		)
	}}
</ul>